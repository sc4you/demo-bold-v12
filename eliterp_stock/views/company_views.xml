<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_form_company_stock" model="ir.ui.view">
        <field name="name">view_form_company_stock</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <notebook position="inside">
                <page string="Inventario">
                    <group col="4">
                        <field name="sequence_inventory_id" options="{'no_open': True, 'no_create': True}"/>
                    </group>
                </page>
            </notebook>
        </field>
    </record>
</odoo>

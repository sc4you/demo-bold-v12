<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <!--Sequences-->
        <record model="ir.sequence" id="sequence_pay_order">
            <field name="name">Orden de pago</field>
            <field name="code">pay.order</field>
            <field name="prefix">ORP/%(year)s/</field>
            <field name="company_id" ref="base.main_company"/>
        </record>
        <record model="ir.sequence" id="sequence_payment_inbound">
            <field name="name">Cobros</field>
            <field name="code">payment.inbound</field>
            <field name="prefix">COBRO/%(year)s/%(month)s/</field>
            <field name="company_id" ref="base.main_company"/>
        </record>
        <record model="ir.sequence" id="sequence_payment_outbound">
            <field name="name">Pagos</field>
            <field name="code">payment.outbound</field>
            <field name="prefix">CE/%(year)s/%(month)s/</field>
            <field name="company_id" ref="base.main_company"/>
        </record>
        <record model="ir.sequence" id="sequence_wire_transfer">
            <field name="name">Transferencia bancaria</field>
            <field name="code">wire.transfer</field>
            <field name="prefix">TRA/%(year)s/%(month)s/</field>
            <field name="company_id" ref="base.main_company"/>
        </record>
    </data>
</odoo>